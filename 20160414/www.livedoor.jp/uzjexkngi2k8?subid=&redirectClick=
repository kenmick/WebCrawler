tpAdtage00fdee4fc.width = 300;
tpAdtage00fdee4fc.height = 250;
tpAdtage00fdee4fc.actvis = -1;
tpAdtage00fdee4fc.envData = "";
tpAdtage00fdee4fc.extData.push("");
