<script type="text/javascript+fk-onload">
    $('.lifeStyle').each(function(){
        var timer = null;
        var $this_box = this;
        var $nav_box =  $('.navBox',this);
        var $hidden_nav = $('.hidden-nav',this);
        var $navBoxes =  $('.navBoxes', this);
        $('.navBox',this).mouseenter(function(){
            var $this = $(this);
            timer = setTimeout(function(){
                var n = $nav_box.index($this);
                $nav_box.removeClass('hover');
                $this.addClass('hover');
                $navBoxes.addClass('fk-hidden');
                $('.navBoxes:eq(' + n + ')', $this_box).removeClass('fk-hidden');
                $hidden_nav.css({'height':'166px'});
            },500);
        });
        $(this).mouseleave(function(){
            clearTimeout(timer);timer=null;
            $nav_box.removeClass('hover');
            $hidden_nav.css({'height':'0px'});
            setTimeout(function(){$navBoxes.addClass('fk-hidden');},200);
        });
        $('.navBox',this).mouseleave(function(){
            clearTimeout(timer);timer=null;
        });
    });
