rack");var $=t.getGlobal("$");var a=t.getService("viewport-monitor");var r={};function n(t){var a=this;a.trackIfVisible=function(){var a=$(t)[i.selector]("["+i.gtrack_cat+"]")[i.read_attr](i.gtrack_cat);var r=$(t)[i.selector]("["+i.gtrack_act+"]")[i.read_attr](i.gtrack_act);var n=$(t)[i.selector]("["+i.gtrack_label+"]")[i.read_attr](i.gtrack_label);if(a!==undefined&&r!==undefined){s.track_events(a,"[nc]imp:"+r,n,null,true)}}}r.addImpressionTracking=function(t){var i=new n(t);a.startWatching({target:t,renderLookahead:0,callback:i.trackIfVisible,watchOnce:true})};return r});Box.Application.addService("viewport-monitor",function(t){"use strict";var i={};var s=0;var a=t.getService("dom");var r;function n(t){var i=a.getViewPosition(t.target,t.renderLookahead);if(i.isPartiallyInViewport()||i.isPartiallyInLookahead()){t.callback();if(t.watchOnce){r.stopWatching(t.target)}}}function o(){for(var t in i){if(i.hasOwnProperty(t)){n(i[t])}}}t.getService("scroll-listener").add({callback:o});r={};r.startWatching=function(t){i[s++]=t;n(t)};r.stopWatching=function(t){if(t instanceof HTMLElement){for(var s in i){if(i.hasOwnProperty(s)&&i[s].target===t){delete i[s];break}}}};return r});Box.Application.addService("scroll-listener",function(t){"use strict";var i=350;var s=0;var a={};var r=t.getService("dom");var n=t.getService("util");function o(t){var i=this;var a={};var o=t.throttleTimeout;this.add=function(t){a[++s]=t;return s};this.remove=function(t){delete a[t]};this.trigger=function(t){if(a.hasOwnProperty(t)){a[t]()}else{for(var i in a){if(a.hasOwnProperty(i)){a[i]()}}}};r.on("scroll resize",n.throttle(i.trigger,o))}var l={};l.add=function(t){if(typeof t.throttleTimeout==="undefined"){t.throttleTimeout=i}var s=a[t.throttleTimeout];if(!s){s=a[t.throttleTimeout]=new o(t)}var r=s.add(t.callback);s.trigger(r);return r};l.remove=function(t){for(var i in a){if(a.hasOwnProperty(i)){a[i].remove(t)}}};return l});Box.Application.addService("seed-user-promo-selector",function(t){"use strict"