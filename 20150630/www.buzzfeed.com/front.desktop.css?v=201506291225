@font-face { font-family: "Proxima Nova"; src: url("/static/fonts/ProximaNova-Semibold-webfont.woff") format("woff"); font-weight: bold; font-style: normal; font-stretch: normal; }
html { font-family: 'Proxima Nova', Helvetica, sans-serif; }

