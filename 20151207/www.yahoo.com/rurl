  url = encodeURIComponent("http://red.sfr.fr/offre-fibre-optique-pas-cher.html");*/

setOmniture();

if(ad==='RED-FORFAIT-3E99' && publisher ==='Yahoo' && ADventori.env.ua.ie){
          ADventori.conf.displayAlt = true;
        ADventori.conf.scenario = 'alt_ie';
}


eadPublisher = publisher;
eadName = 'Display-acquisition-'+'Desktop'+'-'+publisher+'-'+plan;
eadLocation =  emplacement;
eadCreative = ad2;
eadMediaPlan = plan;
eUrl = url+omniture;

if(size==='300x600'||size==='300x250'||size==='728x90'||size==='300x600-EXPAND'||size==='1000x270'||size==='120x600'||size==='160x600'){
  eadCreativeType='IAB-'+size;
} else if (size==='970x250'){
  eadCreativeType='BILLBOARD-'+size;
 }  else {
  eadCreativeType=size;
}


