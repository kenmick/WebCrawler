//»ñÈ¡ÐËÈ¤ÆµµÀÅÅÐò
function contentInit(obj){
    var defaultArr = ["finance","sports","ent","tech","auto","house","book","cul","fashion","dajia","astro","edu","kid","history","games","dy","newssh","rufodao"]; //ËùÓÐÆµµÀÊý×é
    var fixedArr = ["finance","sports","ent"]; //¹Ì¶¨Õ¹¿ªÈý´óÆµµÀ
    var contentArr = ["tech","auto","house","book","cul","fashion","dajia","astro","edu","kid","history","games","dy","newssh","rufodao"]; //È¥³ý¹Ì¶¨Õ¹¿ªÈý´óÆµµÀÊý×é
    var interestArr = []; //ÐËÈ¤ÆµµÀÊý×é
    var interestChanel = ""; //ËÄ´óÆµµÀÖ®ÍâµÄÐËÈ¤ÆµµÀ
    var finalArr = []; //ÎÞ×î¸ßÐËÈ¤µÄ×îÖÕÆµµÀÊý×é

    if(obj.num != "undefined" && obj.num != 0){ //ÓÐÐËÈ¤ÆµµÀ
        for(var i=0; i<obj.num; i++){ //°ÑÐËÈ¤ÆµµÀ·Åµ½Êý×éÀï
            interestArr.push(obj.interest[i].ename);
        }
        for(var i=0; i<interestArr.length; i++){ //ÅÐ¶ÏÐËÈ¤ÆµµÀÖÐÊÇ·ñÓÐËÄ´óÆµµÀÖ®ÍâµÄÆµµÀ
            if(interestArr[i] != "finance" && interestArr[i] != "sports" && interestArr[i] != "ent" && interestArr[i] != "tech"){ //Èç¹ûÓÐ£¬¸ÃÐËÈ¤ÆµµÀ¾ÍÊÇ×î¸ßÐËÈ¤ÆµµÀ
                interestChanel = interestArr[i];
                if(interestChanel == "comic"){ //¶¯ÂþÐËÈ¤×ªÎªÓÎÏ·
                    interestChanel = "games";
                }
                if(interestChanel == "milite"){ //¾üÊÂÐËÈ¤×ªÎªÀúÊ·
                    interestChanel = "history";
                }
                if(interestChanel == "digi"){ //ÊýÂëÐËÈ¤×ªÎª¿Æ¼¼
                    interestChanel = "tech";
                }
                for(var n=0; n<contentArr.length; n++){ //°Ñ×î¸ßÐËÈ¤ÆµµÀ´ÓËùÓÐÆµµÀÊý×éÒÆ³ö
                    if(interestChanel == contentArr[n]){
                        contentArr.splice(n,1);
                        break;
                    }
                }
                break;
            }
        }
        if(interestChanel != ""){ //Èç¹ûÓÐËÄ´óÆµµÀÖ®ÍâµÄÐËÈ¤ÆµµÀ
            finalArr = fixedArr.push(interestChanel);
            finalArr = fixedArr.concat(contentArr); //ºÏ²¢ÐËÈ¤ÆµµÀ
        }else{
            finalArr = ["finance","sports","ent","tech","auto","house","book","cul","fashion","dajia","astro","edu","kid","history","games","dy","newssh","rufodao"];
        }
    }else{ //Ã»ÓÐÐËÈ¤ÆµµÀ
        finalArr = defaultArr;
    }
    orderInit(finalArr); //°å¿éÅÅÐò
    orderCurrentArr = finalArr;
    //Ìí¼Ó¶©ÖÆÊÂ¼þ
    orderEvent();
}

