<script type="text/javascript">
	var googletag = googletag || {};
	googletag.cmd = googletag.cmd || [];

	(function() {
		var gads = document.createElement('script'),
			useSSL = 'https:' == document.location.protocol,
			node = document.getElementsByTagName('script')[0];

		gads.async = true;
		gads.type = 'text/javascript';
		gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
		node.parentNode.insertBefore(gads, node);
	})();
</script>

<script>
	(function () {
		// asynchronously define ad slots for google publisher tags
		googletag.cmd.push(function() {
			if (window.upstream.pageType === 'home') {
				// home page ads
				defineLeaderboard('top', '_home/HOME_TOP_LEADERBOARD', '');
				defineBoxAd('top', '_home/HOME_TOP_BOXAD', '');
				defineBoxAd('bottom', '_home/HOME_BOTTOM_BOXAD', '');
			} else {
				// article page ads
				if (window.upstream.isMobileScreenSize) {
					defineLeaderboard('top', '_article/ARTICLE_TOP_LEADERBOARD', '-mobile');
					defineBoxAd('top', '_article/ARTICLE_TOP_BOXAD', '-mobile');
				} else {
					defineLeaderboard('top', '_article/ARTICLE_TOP_LEADERBOARD', '-desktop');
					defineBoxAd('top', '_article/ARTICLE_TOP_BOXAD', '-desktop');
				}
				defineBoxAd('bottom', '_article/ARTICLE_BOTTOM_BOXAD', '');
			}

			// Loads Krux Segments into GPT, must run before enableServices && enableSingleRequest
			addKruxSegments();
			googletag.pubads().enableSingleRequest();
			googletag.enableServices();
		});

		/**
		 * Set up a leaderboard ad with parameters
		 * @param {string} position Options are "top" or "bottom", i.e. where the ad exists in the page
		 * @param {string} pathSuffix
		 * @param {string} idSuffix
		 */
		function defineLeaderboard(position, pathSuffix, idSuffix) {
			var sizeMapping,
				tag;

			idSuffix = idSuffix || '';

			sizeMapping = googletag.sizeMapping()
				.addSize([970, 200], [[728, 90], [970, 250]])
				.addSize([753, 200], [[728, 90]])
				.addSize([0,0], [[300, 250]])
				.build();

			tag = googletag.defineSlot(
				'/5441/wka.fandom/' + pathSuffix,
				[[728, 90], [970, 250]],
				'gpt-' + position + '-leaderboard' + idSuffix
			)
				.defineSizeMapping(sizeMapping)
				.setTargeting('loc', position)
				.setTargeting('pos', position + '_leaderboard');

			addCommonSettings(tag);
			addPostDataTargeting(tag);
		}

		/**
		 * Set up a box ad with parameters
		 * @param {string} position Options are "top" or "bottom", i.e. where the ad exists in the page
		 * @param {string} pathSuffix
		 * @param {string} idSuffix
		 */
		function defineBoxAd(position, pathSuffix, idSuffix) {
			var sizeMapping,
				tag;

			idSuffix = idSuffix || '';

			sizeMapping = googletag.sizeMapping()
				.addSize([0, 0], [[300, 250]])
				.addSize([768, 200], [[300, 1050], [300, 600], [300, 250]])
				.build();

			tag = googletag.defineSlot(
				'/5441/wka.fandom/' + pathSuffix,
				[[300, 250],[300, 1050], [300, 600]],
				'gpt-' + position + '-boxad' + idSuffix
			)
				.defineSizeMapping(sizeMapping)
				.setTargeting('loc', position)
				.setTargeting('pos', position + '_boxad');

			addCommonSettings(tag);
			addPostDataTargeting(tag);
		}

		/**
		 * Add targeting to an ad based on page metadata
		 * @param tag
		 */
		function addPostDataTargeting(tag) {
			var postData = window.upstream.post;

			if (window.upstream.isHomePage) {
				tag.setTargeting('vertical', 'home');

			} else if (postData.verticals.length) {
				tag.setTargeting('vertical', postData.verticals);
			}

			if (postData.topics.length) {
				tag.setTargeting('topic', postData.topics);
			}

			if (postData.tags.length) {
				tag.setTargeting('tags', postData.tags);
			}
		}

		/**
		 * Apply settings to an ad slot that are common for all tags
		 * @param tag
		 */
		function addCommonSettings(tag) {
			tag.setCollapseEmptyDiv(true)
				.addService(googletag.pubads())
				.setTargeting('src', 'gpt')
		}

		function addKruxSegments() {
			if (window.Krux && Krux.segments && Krux.user) {
				googletag.pubads().setTargeting("ksg", Krux.segments);
				googletag.pubads().setTargeting("kuid", Krux.user);
			}
		}

	})();
</script>
