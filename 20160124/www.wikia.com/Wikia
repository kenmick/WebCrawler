		// add state classes for css
		(function(html){
			// js enabled
			html.classList.remove('no-js');
			html.classList.add('js');

			// touch devices
			if (window.upstream.isTouchDevice) {
				html.classList.add('touch-device');
			} else {
				html.classList.add('non-touch-device');
			}
		})(document.documentElement);

		(function () {
	'use strict';
	var referrer = document.referrer;
	if (shouldWelcome()) {
		// Show lockup
		window.upstream.showWelcomeLockup = true;
		var maxAge = 60 * 60 * 24 * 365;
		document.cookie = 'returning_fandom_user=1; max-age=' + maxAge + '; path=/';
	}

	function shouldWelcome() {
		// Always return false, disable ahead of branding updates official launch
		return false;

		var isWikia = referrer.match(/wikia\.com/);
		var isNonFandomReferrer = !referrer.match(/(www\.wikia|fandom\.wikia?)/);
		var returningFandomUser = document.cookie.indexOf('returning_fandom_user') > -1;

		if (!returningFandomUser && isWikia && isNonFandomReferrer) {
			return true;
		}

		return false;
	}
}());
	</script>

