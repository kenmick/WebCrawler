    props = this.css(prefix + 'transition-property').split(', ')

    if (!props.length) {
      return this
    }

    if (!jumpToEnd) {
      style = document.defaultView.getComputedStyle(this.get(0), '')
      for (i = 0; i < props.length; i++) {
        this.css(props[i], style.getPropertyValue(props[i]))
      }
    }

