    var boucle = 0;
	
    function Animation(){
		
       $(document).ready(function(){

       setTimeout(function(){
          $('#bonplan').animate({ opacity: '0' }, 0, 'ease-in');
          $('#bulleorange').animate({ opacity: '1', transform: 'scale(0.737)' }, 700, 'ease-in');
        }, 2100);

       setTimeout(function(){
          $('#bulleorange').animate({ opacity: '1', transform: 'scale(0.332)' }, 200, 'ease-in');
        }, 2800);

       setTimeout(function(){
          $('#cadreorange').animate({ transform: 'translate(-101px,0px)' }, 400, 'ease-in');
          $('#bulleorange').animate({ transform: 'scale(0.553)', left: '319px', top: '6px' }, 400, 'ease-in');
        }, 3000);
		
       setTimeout(function(){
          $('#bulleorange').animate({ transform: 'scale(1)', left: '101px', top: '4px' }, 500, 'ease-in');
        }, 3700);
		
       setTimeout(function(){
          $('#date').animate({ opacity: '1' }, 300, 'ease-in');
        }, 3900);
		
       setTimeout(function(){
          $('#dixneufeuros').animate({ opacity: '1' }, 0, 'ease-in');
          $('#pourlesnouveaux').animate({ opacity: '1' }, 200, 'ease-in');
          $('#jenprofite').animate({ transform: 'translate(-116px,0px)' }, 400, 'ease-in');
        }, 4200);
				
       setTimeout(function(){
          $('#jenprofite').animate({ transform: 'translate(-110px,0px)' }, 100, 'ease-in');
        }, 4600);
				
       setTimeout(function(){
          $('#liveboxnecessaire').animate({ opacity: '1' }, 0, 'ease-in');
        }, 4700);
		
       setTimeout(function(){
          $('#bulletexte1').animate({ opacity: '1', transform: 'translate(0px,-90px)' }, 400, 'ease-in');
        }, 6200);
		
       setTimeout(function(){
          $('#bulletexte2').animate({ opacity: '1', transform: 'translate(83px,0px)' }, 400, 'ease-in');
        }, 7200);
		
       setTimeout(function(){
          $('#bullelivebox').animate({ opacity: '1', transform: 'translate(91px,0px)' }, 400, 'ease-in');
        }, 8200);
		
       setTimeout(function(){
          $('#jenprofite').animate({ transform: 'translate(-116px,0px)' }, 200, 'ease-in');
        }, 10500);
		
       setTimeout(function(){
          $('#jenprofite').animate({ transform: 'translate(-110px,0px)' }, 200, 'ease-in');
        }, 10700);
		
       setTimeout(function(){
          $('#date').animate({ left: '-300px' }, 300, 'ease-in');
          $('#dixneufeuros').animate({ left: '-300px' }, 300, 'ease-in');
          $('#bulleorange').animate({ left: '-300px' }, 300, 'ease-in');
          $('#pourlesnouveaux').animate({ left: '-300px' }, 300, 'ease-in');
          $('#bulletexte1').animate({ transform: 'translate(0px,88px)' }, 300, 'ease-in');
          $('#bulletexte2').animate({ transform: 'translate(83px,-89px)' }, 300, 'ease-in');
          $('#bullelivebox').animate({ top: '95px' }, 300, 'ease-in');
          $('#liveboxnecessaire').animate({ transform: 'translate(168px,0px)' }, 300, 'ease-in');
          $('#jenprofite').animate({ transform: 'translate(168px,0px)' }, 300, 'ease-in');
       }, 13100);
		
       setTimeout(function(){
          $('#mentions').animate({ opacity: '1' }, 400, 'easeInOut');
          $('#logo').animate({ opacity: '1' }, 0, 'easeInOut');
          $('#cliquerici').animate({ opacity: '1' }, 0, 'easeInOut');
       }, 13700);
		
       setTimeout(function(){
          $('#cliquerici').animate({ transform: 'scale(1.1)' }, 100, 'easeInOut');
       }, 14800);
		
       setTimeout(function(){
          $('#cliquerici').animate({ transform: 'scale(1)' }, 100, 'easeInOut');
       }, 14900);
		
        /* Ã©cran de fin */
		
       setTimeout(function(){
           if(boucle < 0){
			   $("div").attr('style', null);
		       boucle=boucle+1;
		       Animation();
		   }else{
		       $('#mentions').animate({ opacity: '0' }, 0, 'ease-in');
		       $('#logo').animate({ opacity: '0' }, 0, 'ease-in');
		       $('#cliquerici').animate({ opacity: '0' }, 0, 'ease-in');
		       $('#date').animate({ opacity: '1', left: '17px' }, 0, 'ease-in');
		       $('#bulleorange').animate({ opacity: '1', left: '101px' }, 0, 'ease-in');
		       $('#dixneufeuros').animate({ opacity: '1', left: '109px' }, 0, 'ease-in');
		       $('#pourlesnouveaux').animate({ opacity: '1', left: '194px' }, 0, 'ease-in');
		       $('#bulletexte1').animate({ opacity: '1', transform: 'translate(0px,-89px)' }, 0, 'ease-in');
		       $('#bulletexte2').animate({ opacity: '1', transform: 'translate(83px,0px)' }, 0, 'ease-in');
		       $('#bullelivebox').animate({ opacity: '1', top: '7px' }, 0, 'ease-in');
		       $('#jenprofite').animate({ opacity: '1', transform: 'translate(-110px,0px)' }, 0, 'ease-in');
		       $('#liveboxnecessaire').animate({ opacity: '1', transform: 'translate(0px,0px)' }, 0, 'ease-in');
           }
       }, 16900);


        /* Mentions de l'offre */		

		$('#mentions').on('mouseenter', function() {
			$('#mentions').animate({ translateX: '-484px', translateZ: 0 }, 400, 'easeInOut');
		});

		$('#mentions').on('mouseleave', function() {
			$('#mentions').animate({translateZ: 0 }, 400, 'easeInOut');
		});

      });
    }

  </script>



