  function loadScript(script) {
    var deferred = Q.defer();
    var scriptEl = document.createElement('script');
    scriptEl.setAttribute('type', 'text/javascript');
    scriptEl.setAttribute('src', script);
    scriptEl.onload = function () {
      deferred.resolve();
    };
    document.body.appendChild(scriptEl);
    return deferred.promise;
  }

  EventManager.observe_once(
    "acl:load:done",
    function () {
      var u = new BF_User();
      if (u) {
        u = u.getUserInfo();
      }
      if (u && (u.p_admin === 'true' || u.p_editor_admin === 'true' || u.p_developer_admin === 'true' || u.p_partner_admin === 'true' || u.f_ad === 'true' || u.f_partner === 'true')) {
        var scriptEl = document.createElement('script');
        scriptEl.setAttribute('type', 'text/javascript');
        scriptEl.setAttribute('src', 'http://s3-ak.buzzfed.com/static/js/public/q/q.js');
        scriptEl.onload = function () {
        Q.all([
                loadScript('http://s3-ak.buzzfed.com/static/js/html2canvas/html2canvas.js'),
                loadScript('http://s3-ak.buzzfed.com/static/js/t3/services/user.js?v=1447955162'),
                loadScript('http://s3-ak.buzzfed.com/static/js/t3/services/image-proxier.js?v=1447955162'),
